    "babel-preset-mobx": "^2.0.0",
    "babel-preset-react-app": "^9.1.2",
    "bootstrap": "^4.5.2",
    "form-data": "^3.0.0",
    "mobx": "^5.15.6",
    "mobx-react": "^6.3.0",
    "next": "9.5.2",
    "next-mobx-wrapper": "^0.4.0",
    "react": "16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "16.13.1",
    "react-hooks": "^1.0.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.11.1",
